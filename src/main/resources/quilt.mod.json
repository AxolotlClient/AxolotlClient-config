{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.axolotlclient",
		"id": "axolotlclientconfig",
		"version": "${version}",
		"metadata": {
			"name": "AxolotlClientConfig",
			"description": "Config lib for AxolotlClient",
			"contributors": {
				"moehreag": "Owner",
				"YakisikliBaran": "Contributor",
				"DragonEggBedrockBreaking": "Contributor",
				"TheKodeToad": "Contributor"
			},
			"contact": {
				"homepage": "https://axolotlclient.github.io/",
				"issues": "https://github.com/AxolotlClient/Axolotlclient-config/issues",
				"sources": "https://github.com/Axolotlclient?tab=repositories"
			},
			"icon": "assets/axolotlclient/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"modmenu": [
				"io.github.axolotlclient.AxolotlclientConfig.screen.ModMenuConfigScreens"
			]
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.16.0-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=1.0.0-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19"
			}
		]
	},
	"mixin": "axolotlclientconfig.mixins.json",
	"access_widener": "axolotlclientconfig.accesswidener",
	"minecraft": {
		"environment": "client"
	}
}
